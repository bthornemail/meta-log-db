{
  "name": "meta-log-db",
  "version": "1.2.0",
  "description": "Native database package for Meta-Log (ProLog, DataLog, R5RS)",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "browser": "dist/browser/index.js",
  "module": "dist/browser/index.js",
  "exports": {
    ".": {
      "node": "./dist/index.js",
      "browser": "./dist/browser/index.js",
      "default": "./dist/index.js"
    },
    "./browser": {
      "browser": "./dist/browser/index.js",
      "default": "./dist/browser/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "build:browser": "rollup -c rollup.config.js && node scripts/verify-browser-build.js",
    "build:all": "npm run build && npm run build:browser",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build:all",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "verify:browser": "node scripts/verify-browser-build.js"
  },
  "keywords": [
    "meta-log",
    "prolog",
    "datalog",
    "r5rs",
    "jsonl",
    "canvasl",
    "database",
    "logic-programming",
    "sparql",
    "shacl",
    "rdf",
    "typescript"
  ],
  "author": {
    "name": "Brian Thorne",
    "url": "https://github.com/bthornemail"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bthornemail/meta-log-db.git"
  },
  "bugs": {
    "url": "https://github.com/bthornemail/meta-log-db/issues"
  },
  "homepage": "https://github.com/bthornemail/meta-log-db#readme",
  "license": "MIT",
  "dependencies": {
    "ethers": "^6.0.0",
    "orga": "^4.5.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^30.2.0",
    "rollup": "^4.9.0",
    "ts-jest": "^29.4.5",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
